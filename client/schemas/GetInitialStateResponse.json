{
  "$id": "https://example.com/address.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "config",
  "description": "config object for POST request",
  "type": "object",
  "properties": {
    "requestType": {
      "description": "the object type should be a new board",
      "type": "string",
      "pattern": "^getInitialState$"
    },
    "created": {
      "description": "there should be a created response",
      "type": "boolean"
    }
  },
  "required":["requestType", "created"],
  "additionalProperties": true
}